import java.util.Scanner;
public class calculator {
    public static void main (String[]args){
        int a,b,c;
        System.out.print("Enter any Two numbers:");
        Scanner s= new Scanner(System.in);
        a=s.nextInt();
        b=s.nextInt();
        c=a+b;
        System.out.print("Sum of two Numbers:"+c);}}
